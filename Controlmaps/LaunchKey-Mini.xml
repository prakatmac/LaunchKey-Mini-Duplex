<Device>
  <Name>LaunchKey Mini</Name>
  <Author>Pramukta Kumar</Author>
  <Description>Controlmap for LaunchKey Mini</Description>
  <Parameters>
    <Row>
      <Column>
          <Group name="HorizontalScrollSwitches" orientation="horizontal" columns="2">
            <Param value="CC#106" type="button" name="Left" maximum="127" minimum="0"/>
            <Param value="CC#107" type="button" name="Right" maximum="127" minimum="0"/>
          </Group>       
      </Column>
      <Column>
        <Row>
          <Group name="Encoders" orientation="horizontal" columns="8">
            <Param value="CC#21" type="dial" name="Encoder #1" maximum="127" minimum="0"/>
            <Param value="CC#22" type="dial" name="Encoder #2" maximum="127" minimum="0"/>
            <Param value="CC#23" type="dial" name="Encoder #3" maximum="127" minimum="0"/>
            <Param value="CC#24" type="dial" name="Encoder #4" maximum="127" minimum="0"/>
            <Param value="CC#25" type="dial" name="Encoder #5" maximum="127" minimum="0"/>
            <Param value="CC#26" type="dial" name="Encoder #6" maximum="127" minimum="0"/>
            <Param value="CC#27" type="dial" name="Encoder #7" maximum="127" minimum="0"/>
            <Param value="CC#28" type="dial" name="Encoder #8" maximum="127" minimum="0"/>
          </Group>
        </Row>
        
        <Row>
          <Group name="UpperSwitches" orientation="horizontal" columns="8">
            <Param value="C-7" type="button" name="Switch 1,1" maximum="127" minimum="0"/>
            <Param value="C#7" type="button" name="Switch 1,2" maximum="127" minimum="0"/>
            <Param value="D-7" type="button" name="Switch 1,3" maximum="127" minimum="0"/>
            <Param value="D#7" type="button" name="Switch 1,4" maximum="127" minimum="0"/>
            <Param value="E-7" type="button" name="Switch 1,5" maximum="127" minimum="0"/>
            <Param value="F-7" type="button" name="Switch 1,6" maximum="127" minimum="0"/>
            <Param value="F#7" type="button" name="Switch 1,7" maximum="127" minimum="0"/>
            <Param value="G-7" type="button" name="Switch 1,8" maximum="127" minimum="0"/>         
          </Group>
        </Row>
        <Row>
          <Group name="LowerSwitches" orientation="horizontal" columns="8">
            <Param value="E-8" type="button" name="Switch 2.1" maximum="127" minimum="0"/>
            <Param value="F-8" type="button" name="Switch 2,2" maximum="127" minimum="0"/>
            <Param value="F#8" type="button" name="Switch 2,3" maximum="127" minimum="0"/>
            <Param value="G-8" type="button" name="Switch 2,4" maximum="127" minimum="0"/>
            <Param value="G#8" type="button" name="Switch 2,4" maximum="127" minimum="0"/>
            <Param value="A-8" type="button" name="Switch 2,6" maximum="127" minimum="0"/>
            <Param value="A#8" type="button" name="Switch 2,7" maximum="127" minimum="0"/>
            <Param value="B-8" type="button" name="Switch 2,8" maximum="127" minimum="0"/>          
          </Group>
        </Row>
        <Row>
          <Group name="Keyboard" orientation="horizontal" >
            <Param value="|" type="keyboard" name="" size="0.75" velocity_enabled="true" aspect="1.4" range="25" maximum="127" minimum="0"/>
          </Group>
        </Row>
      </Column>
      <Column>
          <Group name="CircleSwitches" orientation="horizontal" columns="1">
            <Param value="G#7" type="button" name="Circle Up" maximum="127" minimum="0"/>
            <Param value="C-9" type="button" name="Circle Down" maximum="127" minimum="0"/>
          </Group>
      </Column>
      <Column>
          <Group name="VerticalScrollSwitches" orientation="horizontal" columns="1">
            <Param value="CC#104" type="button" name="Up" maximum="127" minimum="0"/>
            <Param value="CC#105" type="button" name="Down" maximum="127" minimum="0"/>
          </Group>
      </Column>
    </Row>

  </Parameters>
</Device>
